{
  "name": "apigee-templater",
  "version": "3.0.0",
  "description": "A CLI to template and convert apigee proxies between ZIP, JSON and YAML formats.",
  "homepage": "https://github.com/tyayers/apigee-templater",
  "bugs": "https://github.com/tyayers/apigee-templater/issues",
  "exports": "./dist/cli.js",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "bin": {
    "apigee-templater": "./bin/apigee-templater.js"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
