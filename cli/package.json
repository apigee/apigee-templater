    "deploy": "npm run build && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "tayers",
  "license": "ISC",
  "dependencies": {
    "arg": "^5.0.1",
    "chalk": "^5.3.0",
    "esm": "^3.2.25",
    "inquirer": "^8.2.0"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.0",
    "@types/node": "^20.9.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
