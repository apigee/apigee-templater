{
  "name": "test-exvars-proxy",
  "profile": "default",
  "endpoints": [
    {
      "name": "default",
      "basePath": "/httpbin",
      "target": {
        "name": "default",
        "url": "https://httpbin.org",
        "headers": {
          "Accept-Encoding": "gzip,deflate"
        }
      },
      "quotas": [
        {
          "count": "200",
          "timeUnit": "day"
        }
      ],
      "spikeArrest": {
        "rate": "20s"
      },
      "auth": [
        {
          "type": "apikey"
        }
      ],
      "extensionSteps": [
        {
          "type": "ExtractVariables",
          "name": "extractHeaders",
          "triggers": ["preRequest"],
          "ignoreUnresolvedVariables": true,
          "prefix": "test",        
          "URIPaths": [
            {
              "ignoreCase": true,
              "pattern": "/accounts/{id}" 
            }
          ],
          "queryParams": [
            {
              "name": "id",
              "ignoreCase": true,
              "pattern": "{user_id}" 
            }
          ],
          "headers": [""],
          "formParams": [""],
          "JSONPaths": [""],
          "XMLPaths": [""]
        }       
      ]
    }
  ]
}