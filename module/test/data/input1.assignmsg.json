{
  "name": "test-exvars-proxy",
  "profile": "default",
  "endpoints": [
    {
      "name": "default",
      "basePath": "/httpbin",
      "target": {
        "name": "default",
        "url": "https://httpbin.org",
        "headers": {
          "Accept-Encoding": "gzip,deflate"
        }
      },
      "quotas": [
        {
          "count": "200",
          "timeUnit": "day"
        }
      ],
      "spikeArrest": {
        "rate": "20s"
      },
      "auth": [
        {
          "type": "apikey"
        }
      ],
      "extensionSteps": [
        {
          "type": "AssignMessage",
          "name": "SetHeaders",
          "triggers": ["preRequest"],
          "continueOnError": true,
          "assignTo": "request",
          "ignoreUnresolvedVariables": true,
          "assignVariables": [

          ],
          "add": [

          ],
          "copy": [

          ],
          "remove": [

          ],
          "set": [

          ]
        }       
      ]
    }
  ]
}