{
  "name": "TestProxy",
  "profile": "default",
  "endpoints": [
    {
      "name": "default",
      "basePath": "/httpbin",
      "target": {
        "name": "default",
        "properties":{
          "io.timeout.millis": "20000",
          "connect.timeout.millis": "3000"
        },        
        "url": "https://httpbin.org",
        "headers": {
          "Accept-Encoding": "gzip,deflate"
        }
      },
      "quotas": [
        {
          "count": "200",
          "timeUnit": "day"
        }
      ],
      "spikeArrest": {
        "rate": "20s"
      },
      "auth": [
        {
          "type": "apikey"
        }
      ]
    }
  ]
}