{
  "name": "apigee-templater",
  "version": "3.0.17",
  "description": "Provides tooling for the templating and conversion of Apigee proxies between XML zip, JSON, and Yaml, and providing conversion services through REST, CLI and MCP.",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "scripts": {
    "build": "npx tsc",
    "start": "npm run build && node dist/index.js",
    "test": "npm run build && vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apigee/apigee-templater.git"
  },
  "files": [
    "dist/"
  ],
  "bin": {
    "apigee-templater": "./dist/bin/apigee-templater.js"
  },
  "keywords": [
    "apigee"
  ],
  "author": "tyler.ayers@gmail.com",
  "license": "Apache-2.0",
  "type": "module",
  "bugs": {
    "url": "https://github.com/apigee/apigee-templater/issues"
  },
  "homepage": "https://github.com/apigee/apigee-templater#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.3",
    "arg": "^5.0.2",
    "chalk": "^5.6.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "inquirer": "^12.9.4",
    "xml-js": "^1.6.11",
    "yaml": "^2.8.1",
    "yauzl": "^3.2.0",
    "yazl": "^3.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/yauzl": "^2.10.3",
    "@types/yazl": "^3.3.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
